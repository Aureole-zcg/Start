#include<stdio.h>
void swap(int x, int y)
{
	int temp;
	printf("形参交换前：x=%d;y=%d\n", x, y);
	temp = x;
	x = y;
	y = temp;
	printf("形参交换后：x=%d;y=%d\n", x, y);
}
int main()
{
	int m = 5, n = 2;
	printf("实参交换前：m=%d;n=%d\n", m, n);
	swap(m, n);
	printf("实参交换后：m=%d;n=%d\n", m, n);
	return 0;
}
/*
实参交换前：m=5;n=2
形参交换前：x=5;y=2
形参交换后：x=2;y=5
实参交换后：m=5;n=2
*/
