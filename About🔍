#include<stdio.h>
int main()
{
	int money = 100, price = 120;
	printf("buy?\n");
	printf("%s\n", money >= price ? "yes" : "no");
	//三目运算符，表达式1？表达式2:表达式3
	return 0;
}
/*运算结果：
buy?
no*/
----------------------------------------------------------
#include<stdio.h>
int main()
{
	int a = 10, b = 4;
	printf("%d\n", a > b);
	//10>4,为真，输出1
	printf("%d\n", a <= b);
	//10<=4为假，输出0
	return 0;
}
//关系表达式的值是“真”和“假”，在C程序中用整数1和0表示
----------------------------------------------------------
#include<stdio.h>
int main()
{
	int money = 100, price = 120;
	printf("%d\n", money >= price);
	//money >= price为假，输出0
	return 0;
}
