#include<stdio.h>
int main()
{
	int x, y = 0;
	for (x = 1; x <= 112; x = x + 3)
	{
		y = y + x;
	}
	printf("%d\n", y);
	return 0;
}
------------------------------------------
e.g.
#include<stdio.h>
int main()
{
	int i, sum = 0;
	for (i = 1; i <= 112; i = i + 3)
	{
		sum = sum + i;
	}
	printf("1+4+7+10+...+112=%d\n", sum);
	return 0;
}//作业范例
