#include<stdio.h>
int main()
{
	int x, y;
	for (x = 100;x <= 200;x++)
	{
		for(y=2;y<=x;y++)
		{
			if (x % y ==0)
			{
				break;
			}
		}
		if (x == y)
		{
			printf("%d ", x);
		}
	}
	return 0;
}//输出素数
----------------------------------------------------
//判断是否是素数
#include<stdio.h>
int main()
{
	int a, b;
	printf("输入一个大于3正整数；");
	scanf("%d", &a);
	for (b = 2; b <= a; b++)
		if (a % b == 0) break;
	if (a > b)
	{
		printf("%d不是素数\n", a);
	}
	else
	{
		printf("%d是素数\n", a);
	}
	return 0;
}
