#include<stdio.h>
int main()
{
	int a[10];
	int max, min, i;
	printf("input 10 numbers:");
	for (i = 0; i < 10; i++)
	{
		scanf("%d", &a[i]);
	}
	max = min = a[0];
	
	for (i = 0; i < 10; i++)
	{
		if (a[i] > max) max = a[i];
		if (a[i] < min) min = a[i];
	}
	printf("max=%d\n", max);
	printf("min=%d\n", min);
	return 0;
}
